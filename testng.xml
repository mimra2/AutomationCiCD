<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">


	<groups>
		<run>
			<include name="Purchase" />
		</run>
	</groups>

	<listeners>

		<listener class-name="SeleniumFrameworkDesign.TestComponents.Listeners"></listener>
	</listeners>


	<test thread-count="5" name="Base Test">
		<classes>
			<class name="SeleniumFrameworkDesign.TestComponents.BaseTest" />

		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="ErrorValidation">
		<classes>
			<class name="SeleniumFrameworkDesign.Tests.Errorvalidation" />

		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="SubmitOrderTest">
		<classes>
			<class name="SeleniumFrameworkDesign.Tests.SubmitOrderTest" />

		</classes>
	</test> <!--
	Test -->

</suite> <!--
Suite -->
